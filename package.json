{
  "name": "@septem/tool",
  "version": "0.1.0",
  "description": "A JavaScript utility library",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "docs:api": "typedoc --options typedoc.json",
    "docs:dev": "cd docs-new && npm start",
    "docs:build": "npm run docs:api && cd docs-new && npm run build",
    "docs": "npm run docs:build",
    "lint": "eslint src",
    "lint:tests": "eslint tests",
    "lint:fix": "eslint --fix tests"
  },
  "keywords": [
    "utility",
    "tools",
    "javascript",
    "functions",
    "lodash"
  ],
  "author": "Septem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/septem/tool.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vuepress/plugin-search": "^2.0.0-rc.0",
    "eslint": "^9.21.0",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "jsdoc-plugin-typescript": "^3.2.0",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.6",
    "typedoc": "^0.27.9",
    "typescript": "^5.8.2",
    "vuepress": "^2.0.0-rc.0"
  }
}
