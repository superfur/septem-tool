{
  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".+\\.(js|ts)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "jsdoc-plugin-typescript"],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}